type TodoListItem = 
 record {
   isChecked: bool;
   "text": text;
 };
type TodoListDb = 
 service {
   pull: (nat32, opt nat64, nat) -> (vec ItemDoc) query;
   push: (vec ItemDoc) -> (vec ItemDoc);
 };
type ItemDoc = 
 record {
   deleted: bool;
   id: nat64;
   payload: TodoListItem;
   updatedAt: nat32;
 };
service : () -> TodoListDb
